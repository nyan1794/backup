

public class Person {


	private String name=null;
	private int age=0;
	public String getName(){
		return this.name;

	}
	public int getAge(){
		return this.age;
	}
	public void setName(String name){
		this.name=name;
	}
	public void setAge(int age){
		this.age=age;
	}
	public void showName(){
		System.out.println(name);
	}
	public void showAge(){
		System.out.println(age);
	}
}
