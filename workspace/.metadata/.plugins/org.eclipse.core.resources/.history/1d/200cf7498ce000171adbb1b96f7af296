package sample;

public class suretsu {

	public int [] hairetu={0,0,0};
	public suretsu(int hairetu[0],int hairetu[1],int hairetu[2]){
		this.hairetu[0]=hairetu[0];
		this.hairetu[1]=hairetu[1];
		this.hairetu[2]=hairetu[2];

	}
	public void hyoji(){
		for(int kekka:hairetu){
		System.out.println(kekka);
		}
	}
}
