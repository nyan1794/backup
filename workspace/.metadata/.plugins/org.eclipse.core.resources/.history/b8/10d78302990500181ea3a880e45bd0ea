import java.util.ArrayList;
import java.util.List;

public class ShopDAO {private ShopDTO s = new ShopDTO();

	private List<ShopDTO> list = new ArrayList<ShopDTO>();
	public List<ShopDTO> setDTO(){
		ShopDTO DTO = new ShopDTO();
		for(int i=0;10 > i ;i++){
			s.setAaa("aaa");
			s.setBbb("aaa");
			s.setCcc("aaa");
			list.add(DTO);
			}
		s.setAaa("bbb");
		s.setBbb("bbb");
		s.setCcc("bbb");
		list.add(DTO);
		s.setAaa("ccc");
		s.setBbb("ccc");
		s.setCcc("ccc");
		list.add(DTO);

		return list;
		}

}
